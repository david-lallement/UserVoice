<Project xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
	<Import Project=".\UserVoice.CodeController.props" />
  
	<PropertyGroup>
		<Version>0.0.1</Version>
		<PackageVersion>0.0.1</PackageVersion>
		<Company>Groupe Isagri Services</Company>
		<Copyright>Copyright 2022 Groupe Isagri Services</Copyright>
		<TargetFramework>net6.0</TargetFramework>
		<OutputType>Library</OutputType>
		<IsPackable>true</IsPackable>   
		<LangVersion>10.0</LangVersion>
		<Nullable>enable</Nullable>
		<NeutralLanguage>en</NeutralLanguage>
		<GeneratePackageOnBuild>false</GeneratePackageOnBuild>
		<DocumentationFile>$(OutputPath)$(AssemblyName).xml</DocumentationFile>
		<RestorePackagesWithLockFile>true</RestorePackagesWithLockFile>
		<DisableImplicitNuGetFallbackFolder>true</DisableImplicitNuGetFallbackFolder>
	</PropertyGroup>
  
	<ItemGroup>
		<None Remove="$(AssemblyName).xml" />
		<None Remove="$(AssemblyName).documentation.xml" />
		<None Remove="*.modified" />
	</ItemGroup>
</Project>